<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fr.gerdevstudio.colorcontacts">

    <application
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity android:name=".userlist1stversion.MainActivity">

        </activity>
        <activity

            android:supportsRtl="true"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:name=".userlist2ndVersion.MainActivity"
            android:label="@string/title_usermodel_list"
            android:theme="@style/AppTheme.NoActionBar">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".userlist2ndVersion.UserDetailActivity"
            android:label="@string/title_usermodel_detail"
            android:parentActivityName=".userlist2ndVersion.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".userlist2ndVersion.MainActivity" />
        </activity>
    </application>

</manifest>